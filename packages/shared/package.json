{
  "name": "@flowx/shared",
  "private": true,
  "type": "module",
  "version": "0.0.4",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./builders/*": {
      "import": "./src/builders/*.ts",
      "types": "./src/builders/*.ts"
    },
    "./types/*": {
      "import": "./src/types/*.ts",
      "types": "./src/types/*.ts"
    },
    "./utils/array": {
      "import": "./src/utils/array/index.ts",
      "types": "./src/utils/array/index.ts"
    },
    "./utils/async": {
      "import": "./src/utils/async/index.ts",
      "types": "./src/utils/async/index.ts"
    },
    "./utils/data": {
      "import": "./src/utils/data/index.ts",
      "types": "./src/utils/data/index.ts"
    },
    "./utils/encoding": {
      "import": "./src/utils/encoding/index.ts",
      "types": "./src/utils/encoding/index.ts"
    },
    "./utils/error": {
      "import": "./src/utils/error/index.ts",
      "types": "./src/utils/error/index.ts"
    },
    "./utils/guard": {
      "import": "./src/utils/guard/index.ts",
      "types": "./src/utils/guard/index.ts"
    },
    "./utils/network": {
      "import": "./src/utils/network/index.ts",
      "types": "./src/utils/network/index.ts"
    },
    "./utils/network/*": {
      "import": "./src/utils/network/*.ts",
      "types": "./src/utils/network/*.ts"
    },
    "./utils/object": {
      "import": "./src/utils/object/index.ts",
      "types": "./src/utils/object/index.ts"
    },
    "./utils/string": {
      "import": "./src/utils/string/index.ts",
      "types": "./src/utils/string/index.ts"
    },
    "./utils/time": {
      "import": "./src/utils/time/index.ts",
      "types": "./src/utils/time/index.ts"
    }
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^2.1.1",
    "@vitest/ui": "^2.1.1",
    "typescript": "^5.6.2",
    "vite": "^5.4.5",
    "vitest": "^2.1.1"
  },
  "dependencies": {
    "@sinclair/typebox": "^0.33.11",
    "mongoose": "^8.6.2"
  }
}
