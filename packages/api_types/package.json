{
  "name": "@flowx/api_types",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./generics": {
      "import": "./src/generics.ts",
      "types": "./src/generics.ts"
    },
    "./models/*": {
      "import": "./src/models/*.ts",
      "types": "./src/models/*.ts"
    },
    "./routes/*": {
      "import": "./src/routes/*/index.ts",
      "types": "./src/routes/*/index.ts"
    }
  },
  "devDependencies": {
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "@sinclair/typebox": "^0.33.12",
    "mongoose": "^8.6.3"
  }
}
